{
  "name": "react-boilerplate",
  "version": "5.1.0",
  "description": "A slightly opinionated yet dead simple boilerplate for ReactJS",
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "start": "npm run build:prod && npm run start-prod-server",
    "dev": "npm run clean && cross-env NODE_ENV=development webpack serve --progress --color",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --progress --color",
    "build:gh-pages": "npm run clean && cross-env NODE_ENV=gh-pages webpack --progress --color",
    "start-prod-server": "cross-env NODE_ENV=production node server.js",
    "clean": "rimraf ./docs"
  },
  "author": "Mike Chabot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mikechabot/react-boilerplate/issues"
  },
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "temp-directory": "./.nyc-coverage-temp",
    "report-dir": "./.nyc-coverage"
  },
  "dependencies": {
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/component": "^5.14.1",
    "axios": "^0.21.0",
    "bulma": "^0.9.1",
    "classnames": "^2.2.6",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "p-min-delay": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-json-pretty": "^2.2.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-tabify": "0.1.25",
    "redux": "^4.0.5",
    "redux-entity": "^8.0.20",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/classnames": "^2.2.11",
    "@types/lodash": "^4.14.165",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.12",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "coveralls": "^3.1.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "figlet": "^1.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.5",
    "json-loader": "^0.5.7",
    "loadable-ts-transformer": "^1.0.0-alpha.3",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^7.0.0",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^2.0.2",
    "react-hot-loader": "^4.13.0",
    "react-refresh": "^0.9.0",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.10.1",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
